pipeline {
    agent any

    triggers {
        githubPush()
    }

    stages {
        stage('Git Clone') {
            steps {
                git url: 'https://github.com/Likihes/java-web-app-docker.git', branch: 'feature'
            }
        }
    }

    stage('list files'){
        steps{
            sh "ls -lrt"
        }
    }
}
