pipeline{
    agent any
    tools{
        jdk 'jdk11'
        maven 'maven'

    }
    stages{
        stage('checkout'){
            steps{
               checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Likihes/secretsanta-generator.git']])

            }
        }

        stage('compile'){
            steps{
               sh 'mvn clean compile' 
            }
        }

        stage('junit'){
            steps{
               sh 'mvn clean test'

            }
        }

    }
}